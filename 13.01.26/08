def ws(b,words):
    res=set()
    def dfs(i,j,t,w):
        if '#' in t:return
        if w in words:res.add(w)
        if i<0 or j<0 or i>=len(b) or j>=len(b[0]):return
        c=b[i][j]
        if c not in t:return
        b[i][j]='#'
        for x,y in [(1,0),(-1,0),(0,1),(0,-1)]:
            dfs(i+x,j+y,t[c],w+c)
        b[i][j]=c
