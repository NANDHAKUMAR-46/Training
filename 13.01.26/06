from collections import defaultdict,deque
w=input().split()
g=defaultdict(set)
ind={}
for i in range(len(w)-1):
    for a,b in zip(w[i],w[i+1]):
        if a!=b:
            g[a].add(b)
            ind[b]=ind.get(b,0)+1
            break
q=deque([c for c in g if c not in ind])
res=""
while q:
    u=q.popleft()
    res+=u
    for v in g[u]:
        ind[v]-=1
        if ind[v]==0:q.append(v)
print(res)
