s=input()
sa=list(range(len(s)))
k=1
rank=[ord(c) for c in s]
tmp=[0]*len(s)
while True:
    sa.sort(key=lambda x:(rank[x],rank[x+k] if x+k<len(s) else -1))
    tmp[sa[0]]=0
    for i in range(1,len(s)):
        tmp[sa[i]]=tmp[sa[i-1]]+( (rank[sa[i]],rank[sa[i]+k] if sa[i]+k<len(s) else -1) != (rank[sa[i-1]],rank[sa[i-1]+k] if sa[i-1]+k<len(s) else -1))
    rank=tmp[:]
    if rank[sa[-1]]==len(s)-1:break
    k*=2
print(sa)
