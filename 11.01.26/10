n,m=map(int,input().split())
g=[list(map(int,input().split())) for _ in range(n)]
dp=[[10**9]*m for _ in range(n)]
dp[0][0]=g[0][0]
for i in range(n):
    for j in range(m):
        if i>0: dp[i][j]=min(dp[i][j],dp[i-1][j]+g[i][j])
        if j>0: dp[i][j]=min(dp[i][j],dp[i][j-1]+g[i][j])
print(dp[-1][-1])
